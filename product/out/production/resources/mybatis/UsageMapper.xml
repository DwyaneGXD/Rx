<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.oasis.rx.product.dao.UsageDao">

    <select id="findIsRepeat" resultType="com.oasis.rx.product.entity.UsageEntity">
        SELECT usage_name
        FROM t_usage
        WHERE
        usage_name =#{arg0} and is_delete =0;
    </select>

    <delete id="deleteId" parameterType="java.lang.String">
        DELETE FROM t_usage
        WHERE id =#{arg0}
    </delete>

</mapper>